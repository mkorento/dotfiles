alias ga="git add"
alias gb="git branch"
alias gst"=git status"
alias gd="git diff"
alias gdc="git diff --cached"
alias gl="git pull"
alias gp="git push"
alias gc="git commit -v"
alias gc!="git commit -v --amend"
alias gco="git checkout"
alias gcm="git checkout master"
alias gre="git reset"
alias grv="git remote -v"

